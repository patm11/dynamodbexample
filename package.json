{
  "name": "dynamodbexample",
  "version": "1.0.0",
  "description": "Example app that uses DynamoDB",
  "main": "build/Release/index.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "yarn clean && tsc",
    "test": "yarn build && jest test/* --notify --coverage",
    "test:functional": "node functional.js",
    "start:docker": "docker-compose up -d",
    "stop:docker": "docker-compose down",
    "start:express": "node build/Release/src/index.js",
    "start": "yarn start:docker && yarn build && yarn start:express"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patm11/dynamodbexample.git"
  },
  "author": "patm11",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/patm11/dynamodbexample/issues"
  },
  "homepage": "https://github.com/patm11/dynamodbexample#readme",
  "dependencies": {
    "aws-sdk": "^2.828.0",
    "express": "^4.17.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "rimraf": "3.0.2",
    "typescript": "^4.1.3",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4"
  }
}
